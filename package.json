{
  "name": "voronoi-studio",
  "version": "2.0.0",
  "description": "voronoi studio web app to present an aesthetic and purposeful experience",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build:dev": "npm run build -- --watch --mode=development",
    "seed": "node script/seed.js",
    "start": "node server",
    "start:dev": "npm run build:dev & npm run start-server",
    "start:dev:logger": "LOGGING=true npm run start:dev",
    "start:dev:seed": "SEED=true npm run start:dev",
    "start-server": "JWT=shh nodemon server -e html,js,scss --ignore public --ignore client",
    "test": "NODE_ENV=test JWT=a_secret mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" --require @babel/register --require esm ",
    "test:dev": "npm run test -- --watch",
    "test:dev:models": "npm run test:dev -- --grep=model",
    "test:dev:routes": "npm run test:dev -- --grep=route"
  },
  "author": "Fullstack Academy of Code",
  "license": "MIT",
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.3",
    "autoprefixer": "^10.4.19",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "bootstrap": "^5.0.2",
    "compression": "^1.7.3",
    "d3": "^7.0.0",
    "d3-delaunay": "^6.0.2",
    "express": "^4.16.4",
    "gsap": "^3.7.0",
    "history": "^4.9.0",
    "jsonwebtoken": "^8.5.1",
    "material-design-icons": "^3.0.1",
    "morgan": "^1.9.1",
    "pg": "^8.5.1",
    "postcss-cli": "^11.0.0",
    "react-gsap": "^3.2.1",
    "react-scroll": "^1.8.2",
    "react-scrollmagic": "^2.3.0",
    "react-simple-sidenav": "^2.0.0",
    "reactstrap": "^8.9.0",
    "sequelize": "^6.3.5",
    "tailwindcss": "^3.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.10",
